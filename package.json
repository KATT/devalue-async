{
	"name": "devalue-async",
	"version": "0.7.2",
	"description": "Extension of the great devalue-package with ability to handle async values",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/KATT/devalue-async.git"
	},
	"license": "MIT",
	"author": {
		"name": "KATT",
		"url": "https://katt.dev"
	},
	"type": "module",
	"exports": {
		".": {
			"types": {
				"import": "./lib/index.d.ts",
				"require": "./lib/index.d.cts"
			},
			"import": "./lib/index.js",
			"require": "./lib/index.cjs"
		}
	},
	"files": [
		"LICENSE.md",
		"README.md",
		"lib",
		"package.json",
		"src",
		"!**/*.test.*",
		"!**/test.*"
	],
	"scripts": {
		"build": "tsdown",
		"format": "prettier . --write",
		"lint": "run-p lint:* --print-label",
		"lint:eslint": "eslint . --max-warnings 0",
		"lint:knip": "knip",
		"lint:md": "markdownlint \"**/*.md\" \".github/**/*.md\" --rules sentences-per-line",
		"lint:packages": "pnpm dedupe --check",
		"lint:spelling": "cspell \"**\" \".github/**/*\"",
		"prepare": "husky",
		"test": "vitest",
		"tsc": "tsc"
	},
	"lint-staged": {
		"*": "prettier --ignore-unknown --write"
	},
	"devDependencies": {
		"@eslint-community/eslint-plugin-eslint-comments": "4.5.0",
		"@eslint/js": "9.38.0",
		"@release-it/conventional-changelog": "10.0.0",
		"@types/eslint-plugin-markdown": "2.0.2",
		"@types/node": "22.18.0",
		"@vitest/coverage-v8": "3.2.0",
		"@vitest/eslint-plugin": "1.3.3",
		"console-fail-test": "0.5.0",
		"cspell": "9.2.0",
		"devalue": "^5.1.1",
		"eslint": "9.38.0",
		"eslint-plugin-jsdoc": "61.1.0",
		"eslint-plugin-jsonc": "2.21.0",
		"eslint-plugin-markdown": "5.1.0",
		"eslint-plugin-n": "17.23.0",
		"eslint-plugin-package-json": "0.54.0",
		"eslint-plugin-perfectionist": "4.15.0",
		"eslint-plugin-regexp": "2.10.0",
		"eslint-plugin-yml": "1.19.0",
		"husky": "9.1.7",
		"knip": "5.66.0",
		"lint-staged": "16.2.0",
		"markdownlint": "0.39.0",
		"markdownlint-cli": "0.45.0",
		"npm-run-all2": "8.0.4",
		"prettier": "3.6.0",
		"prettier-plugin-curly": "0.3.1",
		"prettier-plugin-packagejson": "2.5.10",
		"prettier-plugin-sh": "0.18.0",
		"release-it": "19.0.2",
		"sentences-per-line": "0.3.0",
		"tsdown": "^0.15.0",
		"typescript": "5.9.2",
		"typescript-eslint": "8.41.0",
		"vitest": "3.2.0"
	},
	"peerDependencies": {
		"devalue": "^5.0.0"
	},
	"packageManager": "pnpm@10.18.0",
	"engines": {
		"node": ">=18.3.0"
	},
	"publishConfig": {
		"provenance": true
	}
}
